    }
   ],
   "source": [
    "x = var('x')\n",
    "h = sin(x)/cos(x)^2\n",
    "h.integral(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "mu, nu, a = var('mu nu a')\n",
    "g_2 = -1/(2*mu)*(x+nu)^2 + mu*a"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "1/3*(3*sqrt(2)*a*mu - sqrt(2)*a)*sqrt(a*mu)"
      ]
     },
     "execution_count": 3,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "integrate(g_2, (x, -nu, +sqrt(2*mu*a)-nu)).full_simplify()"
   ]
  },
